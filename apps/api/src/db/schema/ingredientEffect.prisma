model IngredientEffect {
  ingredientId Int
  skinTypeId   Int
  Effect       Effects
  ingredient  Ingredients @relation(fields: [ingredientId], references: [id])
  skinType     SkinType    @relation(fields: [skinTypeId], references: [id])

  @@id([ingredientId, skinTypeId])
}

enum Effects {
  GOOD
  BAD
}
