{
  "name": "glow-studio",
  "version": "1.0.0",
  "description": "Projecto API web para la materia Practicas Profecionalizantes",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && pnpm tsc",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "dev": "tsx --watch src/index.ts",
    "db:migrate": "prisma migrate dev --schema src/db/schema",
    "db:generate": "prisma generate --no-engine --schema src/db/schema"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Memolxz/glow-studio.git"
  },
  "bugs": {
    "url": "https://github.com/Memolxz/glow-studio/issues"
  },
  "packageManager": "pnpm@10.8.1",
  "type": "commonjs",
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.0.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "concurrently": "^9.1.2",
    "prisma": "^6.12.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "schema": "src/db/schema"
  }
}
